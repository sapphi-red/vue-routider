{
  "version": "0.10.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsup src/index.ts --watch src",
    "build": "tsup src/index.ts --dts --sourcemap --format esm,cjs,iife",
    "test": "jest",
    "lint": "eslint --cache \"{src,test}/**/*.ts\"",
    "prepare": "husky install"
  },
  "name": "vue-routider",
  "description": "Make vue-router type safe. (Not compatible with vue-router@3)",
  "author": "sapphi-red",
  "keywords": [
    "vue",
    "router",
    "routing",
    "vue-router",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/sapphi-red/vue-routider/issues"
  },
  "homepage": "https://github.com/sapphi-red/vue-routider#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vue/test-utils": "^2.0.0-rc.2",
    "es-jest": "^1.5.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "tsup": "^5.11.6",
    "typescript": "~4.1.3",
    "vue": "^3.0.6",
    "vue-router": "^4.0.4"
  },
  "dependencies": {
    "tiny-warning": "^1.0.3"
  },
  "peerDependencies": {
    "vue": "^3.0.6",
    "vue-router": "^4.0.0"
  },
  "sideEffects": false,
  "browserslist": [
    "last 1 chrome version"
  ]
